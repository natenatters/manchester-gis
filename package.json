{
  "name": "manchester-gis",
  "version": "1.0.0",
  "type": "module",
  "description": "Historical GIS mapping for Greater Manchester (1086 - present)",
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp -r dist/manchester-gis/cesium dist/cesium",
    "build:data": "python3 data/scripts/build_czml.py",
    "preview": "vite preview"
  },
  "keywords": ["gis", "cesium", "history", "manchester", "roman", "domesday"],
  "author": "Nate Natters",
  "license": "MIT",
  "dependencies": {
    "cesium": "^1.137.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "vite": "^7.3.1",
    "vite-plugin-cesium": "^1.2.23"
  }
}
